<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Doze: does this device support STATE_DOZE?  -->
    <bool name="doze_display_state_supported">false</bool>

    <!-- Whether show refresh rate QS tile -->
    <bool name="config_showRefreshRateQsTile">true</bool>

    <!-- Bottom margin size when fingerprint in display is active -->
    <dimen name="keyguard_indication_margin_bottom">6dp</dimen>

    <!-- Whether usage of the proximity sensor during doze is supported -->
    <bool name="doze_proximity_sensor_supported">false</bool>

    <!-- Whether the proximity sensor should be checked before pulsing -->
    <bool name="doze_proximity_check_before_pulse">false</bool>

    <!-- Doze: does the double tap sensor need a proximity check? -->
    <bool name="doze_double_tap_proximity_check">false</bool>
</resources>
